if game.PlaceId == 3956818381 then
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "0xYzHub| ", HidePremium = false, IntroText = "0xYzHub", SaveConfig = true, ConfigFolder = "TutorialConfig"})


--Values

_G.autoTap = true

_G.autoBelt = true

_G.autoHatch = true

_G.autoEvolve = true

_G.autoSword = true

--Functions

function autoSword()
	while _G.autoSword == true do
		-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "buyAllSwords",
    [2] = "Blazing Vortex Island"
}

game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(unpack(args))

		wait(60000)
	end
end



function autoEvolve()
	while _G.autoEvolve == true do
		-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "autoEvolvePets"
}

game:GetService("ReplicatedStorage").rEvents.autoEvolveRemote:InvokeServer(unpack(args))

		wait(15)
	end
end

function autoTap()
	while _G.autoTap == true do
		-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "swingKatana"
}

game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(unpack(args))

		wait(0.01)
	end
end

function autoBelt()
	while _G.autoBelt == true do
		-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "buyAllBelts",
    [2] = "Blazing Vortex Island"
}

game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(unpack(args))

		wait(60000)
	end
end

function autoHatch()
	while _G.autoHatch == true do
		-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "openCrystal",
    [2] = "Infinity Void Crystal"
}

game:GetService("ReplicatedStorage").rEvents.openCrystalRemote:InvokeServer(unpack(args))

		wait(0.9)
	end
end


local FarmTab = Window:MakeTab({
	Name = "main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--FarmTabs

FarmTab:AddToggle({
	Name = "Otomatik Tıkla",
	Default = false,
	Callback = function(Value)
		_G.autoTap = Value
		autoTap()
	end    
})

FarmTab:AddToggle({
	Name = "Otomatik Yumurta Aç",
	Default = false,
	Callback = function(Value)
		_G.autoHatch = Value
		autoHatch()
	end    
})

FarmTab:AddToggle({
	Name = "Otomatik Evolve",
	Default = false,
	Callback = function(Value)
		_G.autoEvolve = Value
		autoEvolve()
	end    
})



local FarmTab = Window:MakeTab({
	Name = "main 2",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

FarmTab:AddButton({
	Name = "Kemer Al",
	Callback = function()
		autoBelt()
  	end    
})

FarmTab:AddButton({
	Name = "Kılıç Al",
	Callback = function()
		autoSword()
  	end    
})

end
OrionLib:Init()
